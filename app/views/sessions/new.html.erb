<h1>Log In</h1>

<%= flash[:error] %>

<%= form_with url: "/sessions" do |f| %>
	<%= f.text_field :username, placeholder: "Username" %><br>
	<%= f.password_field :password, placeholder: "Password" %><br>
	<%= f.submit "Log In" %>
<% end %>

<p>New User? <%= link_to "Sign Up", new_user_path %> now!</p>